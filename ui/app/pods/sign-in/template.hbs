{{bread-crumbs tagName="ol" outputStyle="bootstrap" linkable=true}}
<div class="jumbotron row">
  <div class="col-md-6 col-md-offset-3">
    {{#bs-form model=signInData onSubmit=(action "submit") as |form|}}
      {{form.element controlType="email" label="Почта:" placeholder="ivanov@example.com" property="email" required=false}}
      {{form.element controlType="password" label="Пароль:" placeholder="Пароль" property="password" required=true}}
      {{bs-button block=true defaultText="Войти" type="success" buttonType="submit"}}
    {{/bs-form}}
  </div>
</div>
